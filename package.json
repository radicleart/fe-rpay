{
  "name": "risidio-pay",
  "version": "0.7.87",
  "private": false,
  "files": [
    "dist"
  ],
  "main": "dist/risidio-pay.umd.js",
  "unpkg": "dist/risidio-pay.umd.min.js",
  "module": "dist/risidio-pay.esm.js",
  "browser": {
    "./sfc": "src/risidio-pay.vue"
  },
  "scripts": {
    "build-report": "vue-cli-service build --report",
    "build": "vue-cli-service build",
    "build-stag": "vue-cli-service build --mode staging",
    "build:npm": "vue-cli-service build --mode production --target lib --name risidio-pay src/RisidioPay.vue",
    "serve": "vue-cli-service serve --port=8082",
    "lint": "vue-cli-service lint",
    "build-app": "vue-cli-service build --mode development",
    "build-app-stag": "vue-cli-service build --mode staging",
    "build-app-prod": "vue-cli-service build --mode production",
    "build-wc": "vue-cli-service build --mode development  --target wc --name rpay-entry ./src/RPayEntry.vue --inline-vue",
    "build-stag-wc": "vue-cli-service build --mode staging --target wc --name rpay-entry ./src/RPayEntry.vue --inline-vue",
    "build-prod-wc": "vue-cli-service build --mode production --target wc --name rpay-entry ./src/RPayEntry.vue --inline-vue",
    "test:e2e": "vue-cli-service test:e2e",
    "test:unit": "vue-cli-service test:unit"
  },
  "dependencies": {
    "@stacks/blockchain-api-client": "^0.65.0",
    "@stacks/connect": "^6.2.0",
    "@stacks/encryption": "^2.0.1",
    "@stacks/network": "^2.0.1",
    "@stacks/storage": "^2.0.1",
    "@stacks/transactions": "^2.0.1",
    "@stomp/stompjs": "^4.0.6",
    "axios": "^0.21.1",
    "bn.js": "^5.2.0",
    "c32check": "^1.1.2",
    "data-uri-to-buffer": "^3.0.1",
    "jsontokens": "^3.0.0",
    "luxon": "^2.0.2",
    "regenerator-runtime": "^0.13.9",
    "risidio-rates": "^1.0.10",
    "sockjs-client": "^1.5.1",
    "vue": "^2.6.12",
    "vuex": "^3.6.2"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.31.0",
    "@typescript-eslint/parser": "^4.31.0",
    "@vue/cli-plugin-babel": "^4.5.13",
    "@vue/cli-plugin-eslint": "^4.5.13",
    "@vue/cli-plugin-router": "^4.5.13",
    "@vue/cli-service": "^4.5.13",
    "@vue/eslint-config-standard": "^6.0.0",
    "@vue/eslint-config-typescript": "^7.0.0",
    "@vue/test-utils": "^1.2.0",
    "babel-eslint": "^10.1.0",
    "chai": "^4.3.4",
    "eslint": "^7.26.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-vue": "^7.9.0",
    "vue-template-compiler": "^2.6.12"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "@vue/standard"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {},
    "overrides": [
      {
        "files": [
          "**/__tests__/*.{j,t}s?(x)",
          "**/tests/unit/**/*.spec.{j,t}s?(x)"
        ],
        "env": {
          "mocha": true
        }
      }
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
